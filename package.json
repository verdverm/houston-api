{
  "name": "houston-api",
  "version": "1.0.0",
  "description": "GraphQL API for the Astronomer Platform",
  "scripts": {
    "build": "babel src -d dist && npm run prisma-generate",
    "start": "npm run prisma-deploy && npm run prisma-generate && nodemon --exec babel-node -e js,graphql src/index.js",
    "serve": "node dist/index.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src --ignore-pattern src/lib/generated --ext .js --ext .graphql",
    "prisma-deploy": "prisma deploy --env-file .env",
    "prisma-generate": "prisma generate",
    "playground": "graphql playground",
    "knex": "babel-node node_modules/.bin/knex",
    "migrate-latest": "npm run knex migrate:latest"
  },
  "author": "Astronomer",
  "license": "Apache 2.0",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "apollo-server": "^2.2.2",
    "apollo-server-express": "^2.2.2",
    "bcryptjs": "^2.4.3",
    "commander": "github:astronomerio/commander",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "email-templates": "^5.0.3",
    "express": "^4.16.4",
    "generate-password": "^1.4.1",
    "google-auth-library": "^2.0.2",
    "googleapis": "^36.0.0",
    "graphql": "^14.0.2",
    "graphql-binding": "^2.3.8",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.3",
    "graphql-type-json": "^0.2.1",
    "grpc": "^1.17.0",
    "grpc-caller": "^0.11.0",
    "haikunator": "^2.0.0",
    "handlebars": "^4.0.12",
    "handlebars-layouts": "^3.1.4",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "ms": "^2.1.1",
    "pg": "^7.6.1",
    "prisma-binding": "^2.2.12",
    "prisma-client-lib": "^1.23.2",
    "shortid": "^2.2.14",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "nock": "^10.0.5",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.1",
    "supertest": "^3.3.0"
  }
}
