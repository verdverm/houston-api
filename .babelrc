{
  "presets": ["@babel/env"],
  "plugins": [
    "@babel/plugin-proposal-throw-expressions",
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["./src/lib"],
        "alias": {
            "commander-proto": "commander",
            "resolvers": "./src/resolvers",
            "routes": "./src/routes"
        }
      }
    ]
  ]
}
